{
  "name": "prisma-redis-middleware",
  "main": "src/index.ts",
  "version": "1.1.5",
  "description": "Prisma Middleware for caching results of queries in Redis",
  "engines": {
    "node": "^12.x || ^14.x || ^16.x",
    "npm": ">= 7.x"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asjas/prisma-redis-middleware.git"
  },
  "keywords": [
    "prisma",
    "redis",
    "prisma-caching",
    "redis-caching",
    "prisma-middleware",
    "caching"
  ],
  "author": "A-J Roos <asjas@hey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Asjas/prisma-redis-middleware/issues"
  },
  "homepage": "https://github.com/Asjas/prisma-redis-middleware#readme",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "test": "tap",
    "test:watch": "tap -w",
    "coverage": "npm test -- --coverage-report=lcov",
    "check-types": "tsc",
    "lint": "eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "debug": "4.3.3",
    "ioredis": "4.28.5"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/ioredis": "4.28.8",
    "@types/ioredis-mock": "5.6.0",
    "eslint-config-asjas": "1.0.0",
    "husky": "7.0.4",
    "ioredis-mock": "7.1.0",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "tap": "15.1.6",
    "ts-node": "10.6.0",
    "typescript": "4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "*.+(ts)": [
      "eslint"
    ],
    "**/**/*.+(ts)": [
      "prettier --write",
      "git add"
    ]
  }
}
