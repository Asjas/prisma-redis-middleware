{
  "name": "prisma-redis-middleware",
  "author": "A-J Roos <asjas@hey.com>",
  "version": "2.0.0-0",
  "description": "Prisma Middleware for caching results of queries in Redis",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "module": "dist/prisma-redis-middleware.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "clean": "rimraf dist",
    "prepare": "tsdx build",
    "test": "tap",
    "test:watch": "tap -w",
    "coverage": "npm test -- --coverage-report=lcov",
    "check-types": "tsc",
    "lint": "eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "async-cache-dedupe": "1.2.2",
    "mnemonist": "0.39.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@types/ioredis": "4.28.9",
    "@types/tap": "15.0.6",
    "eslint-config-asjas": "1.0.0",
    "husky": "7.0.4",
    "ioredis": "4.28.5",
    "lint-staged": "12.3.5",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "tap": "16.0.0",
    "ts-node": "10.7.0",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "*.+(ts)": [
      "eslint"
    ],
    "**/**/*.+(ts)": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "dist/prisma-redis-middleware.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/prisma-redis-middleware.esm.js",
      "limit": "50 KB"
    }
  ],
  "engines": {
    "node": "^12.x || ^14.x || ^16.x",
    "npm": "^6.x || ^7.x || ^8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asjas/prisma-redis-middleware.git"
  },
  "keywords": [
    "prisma",
    "redis",
    "prisma-caching",
    "redis-caching",
    "prisma-middleware",
    "caching"
  ],
  "bugs": {
    "url": "https://github.com/Asjas/prisma-redis-middleware/issues"
  },
  "homepage": "https://github.com/Asjas/prisma-redis-middleware#readme"
}
